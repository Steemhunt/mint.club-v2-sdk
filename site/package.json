{
  "name": "mint.club-v2-sdk",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run link-sdk && vocs dev",
    "build": "vocs build",
    "preview": "vocs preview",
    "link-sdk": "npx bun link mint.club-v2-sdk",
    "unlink-sdk": "npx bun unlink mint.club-v2-sdk && npx bun install mint.club-v2-sdk@latest",
    "deploy": "npm run unlink-sdk && vercel build && vercel --prod"
  },
  "dependencies": {
    "@types/react": "latest",
    "mint.club-v2-sdk": "latest",
    "react": "latest",
    "react-dom": "latest",
    "typescript": "latest",
    "vocs": "latest"
  },
  "devDependencies": {
    "vercel": "^33.5.3"
  }
}
